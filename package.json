{
  "name": "sass-variable-loader",
  "version": "0.0.3",
  "description": "Sass variable loader module for webpack",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel ./src -d ./dist",
    "test": "mocha --compilers js:babel-core/register",
    "_test": "babel-node src/test.js",
    "lint": "eslint --ext=.js ./src",
    "prepush": "npm run lint && npm run test",
    "prepublish": "npm run lint && npm run test && npm run build",
    "postpublish": "rimraf dist && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nordnet/sass-variable-loader"
  },
  "keywords": [
    "nordnet",
    "sass",
    "variable",
    "loader"
  ],
  "author": "Gustaf Zetterlund <gustaf.zetterlund@nordnet.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nordnet/sass-variable-loader/issues"
  },
  "homepage": "https://github.com/nordnet/sass-variable-loader/#readme",
  "dependencies": {
    "loader-utils": "^0.2.12",
    "lodash": "^3.10.1",
    "lodash.camelcase": "^4.1.1",
    "node-sass": "^3.4.2",
    "query-ast": "^1.0.0",
    "sass-thematic": "^2.0.4",
    "scss-parser": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "husky": "^0.11.5",
    "mocha": "^2.3.4",
    "rimraf": "^2.5.0"
  }
}
